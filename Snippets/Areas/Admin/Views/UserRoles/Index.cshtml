@using Snippets.Areas.Admin.Models
@model IEnumerable<Role>
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Roles
        </th>
    </tr>

@foreach (Role role in Model) {
    <tr>
        <td>
            @role.Name
        </td>
    </tr>
}

</table>